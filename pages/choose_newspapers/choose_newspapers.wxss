.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0 14px 20px 14px;
    box-sizing: border-box;
    /* background-color: #f8f8f8; */
}

.title {
    font-size: 16px;
    font-weight: bold;
}

.subTitle {
    margin-top: 5px;
    font-size: 12px;
}

.newspapersPart {
    margin-top: 30px;
    margin-bottom: 150px;
    display: flex;
    flex-wrap: wrap;
    gap: 5px;
}

.newspaperCard {
    padding: 8px 10px;
    border: 1.5px solid;
    border-color: #c8c8c8;
    border-radius: 20px;
    font-size: 10px;
    color: #cacaca;
}
